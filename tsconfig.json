{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "allowJs": true,
      "sourceMap": true,
      "lib": ["ES6", "DOM"]
    }
  }
  
  